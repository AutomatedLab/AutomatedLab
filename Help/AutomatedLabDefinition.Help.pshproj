    <CmdletObject verb="Add" noun="LabDomainDefinition">
      <Name>Add-LabDomainDefinition</Name>
      <Syntax>
        <string>Add-LabDomainDefinition [-Name] &lt;String&gt; [-AdminUser] &lt;String&gt; [-AdminPassword] &lt;String&gt; [-PassThru]</string>
      </Syntax>
      <GeneralHelp>
        <Synopsis>Add a definition of an Active Directory domain or forest to the lab</Synopsis>
        <Description>Adds a definition of an Active Directory domain or forest name together with username which will be the administrator of the domain as well as the password.
This information is used for AutomatedLab to be able to logon to all machines during and after deployment using domain credentials when needed.</Description>
        <Notes />
        <InputType />
        <InputUrl />
        <InputTypeDescription />
        <ReturnType />
        <ReturnUrl />
        <ReturnTypeDescription />
      </GeneralHelp>
      <ParamSets>
        <CommandParameterSetInfo2 Name="__AllParameterSets" Params="Name AdminUser AdminPassword PassThru Verbose Debug ErrorAction WarningAction InformationAction ErrorVariable WarningVariable InformationVariable OutVariable OutBuffer PipelineVariable" />
      </ParamSets>
      <Parameters>
        <ParameterDescription type="String" varLen="false" required="true" dynamic="false" pipeRemaining="false" pipe="false" pipeProp="true" isPos="true" pos="0" globbing="false">
          <Name>Name</Name>
          <Attributes>
            <string>System.Management.Automation.ParameterAttribute</string>
            <string>System.Management.Automation.ArgumentTypeConverterAttribute</string>
          </Attributes>
          <Aliases />
          <Description>Name of Active Directory domain or forest in FQDN format.</Description>
          <DefaultValue />
        </ParameterDescription>
        <ParameterDescription type="String" varLen="false" required="true" dynamic="false" pipeRemaining="false" pipe="false" pipeProp="true" isPos="true" pos="1" globbing="false">
          <Name>AdminUser</Name>
          <Attributes>
            <string>System.Management.Automation.ParameterAttribute</string>
            <string>System.Management.Automation.ArgumentTypeConverterAttribute</string>
          </Attributes>
          <Aliases />
          <Description>Desired username of the administrator when deploying a new lab or existing user of the administrator if adding to a lab or an existing external (not deployed by AutomatedLab) domain.</Description>
          <DefaultValue />
        </ParameterDescription>
        <ParameterDescription type="String" varLen="false" required="true" dynamic="false" pipeRemaining="false" pipe="false" pipeProp="true" isPos="true" pos="2" globbing="false">
          <Name>AdminPassword</Name>
          <Attributes>
            <string>System.Management.Automation.ParameterAttribute</string>
            <string>System.Management.Automation.ArgumentTypeConverterAttribute</string>
          </Attributes>
          <Aliases />
          <Description>Desired password of the administrator when deploying a new lab or password of an existing administrator if adding to a lab or an existing external (not deployed by AutomatedLab) domain.</Description>
          <DefaultValue />
        </ParameterDescription>
        <ParameterDescription type="SwitchParameter" varLen="false" required="false" dynamic="false" pipeRemaining="false" pipe="false" pipeProp="false" isPos="false" pos="named" globbing="false">
          <Name>PassThru</Name>
          <Attributes>
            <string>System.Management.Automation.ParameterAttribute</string>
            <string>System.Management.Automation.ArgumentTypeConverterAttribute</string>
          </Attributes>
          <Aliases />
          <Description>Whether or not to return the actual created domain definition in the call of this function to enable you to continue working on a pipeline</Description>
          <DefaultValue />
        </ParameterDescription>
      </Parameters>
      <Examples />
